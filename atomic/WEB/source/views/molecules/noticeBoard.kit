<!-- Notice Board -->
<div class="Pos(r) Maw(970px) Mx(a) Mb(15px) jsNoticeBoard">
  <div class="Px(15px) Py(10px) Bgc(#ffffff) Bd(borderSolidRed) Pos(r)">
    <div class="H(100%) W(5px) Bdrs(3px) Pos(a) Start(-1px) T(0) Bgc(c-red-bright) Bdrststart(0) Bdrsbstart(0)"></div>
    <div class="D(f) Ai(c)">
      <div class="Mend(15px)"><i class="fas fa-exclamation-triangle C(c-red-bright)"></i></div>
      <div class="Fx(flexOne)">
        <span><span class="Fw(700)">Warning!</span> KASKUS akan istirahat dulu sebentar Gan, dari jam 00:00 - selesai. <a href="" class="C(c-red-bright) Fw(700) Td(u):h">info di sini</a></span>
      </div>
      <div class="Fx(flexZero)">
        <button>
          <i class="far fa-times C(c-red-bright)"></i>
        </button>
      </div>
    </div>
  </div>
</div>
